    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fpago"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.carta.dx.cartadigital.MetodoPago9">

    <ImageButton
        android:id="@+id/efectivobtn"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_alignTop="@+id/tarjetabtn"
        android:layout_marginEnd="67dp"
        android:layout_toStartOf="@+id/tarjetabtn"
        android:background="@drawable/pagoefectivo" />

    <ImageButton
        android:id="@+id/tarjetabtn"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_alignParentEnd="true"
